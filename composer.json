{
    "name": "itega/laravel-html-dom-parser",
    "description": "Laravel wrapper for the PHP Simple HTML DOM Parser package",
    "keywords": [
        "dom parser",
        "html dom parser",
        "laravel"
    ],
    "require": {
    	"php": ">=5.6",
        "illuminate/support": "^5.*",
        "daguilarm/php-simple-html-dom-parser": "^1.5.*"
    },
    "homepage": "https://github.com/itega/html-dom-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Ammadeuss Raducan",
            "email": "raducan@ammadeuss.ro"
        },
        {
            "name": "Julien Zolli",
            "email": "contact@julienzolli.dev"
        }
    ],
    "autoload": {
        "psr-4": { "Ammadeuss\\LaravelHtmlDomParser\\": "" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ammadeuss\\LaravelHtmlDomParser\\ServiceProvider"
            ],
            "aliases": {
                "HTMLDomParser": "Ammadeuss\\LaravelHtmlDomParser\\Facade"
            }
        }
    }
}
